extends layouts/main.pug
include mixins/videoBlock

block content
    .serch__header
        h3 Searchin for #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos Found
        each item in videos
            +videoBlock({
                id: item.id,
                title: item.title,
                views: item.views,
                videoFile: item.videoFile
            })